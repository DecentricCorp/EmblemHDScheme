    "hyperdb": "git+https://github.com/DecentricCorp/hyperdb.git",
    "hyperdiscovery": "^8.0.0",
    "hyperdrive": "git+https://github.com/DecentricCorp/hyperdrive.git#hyperdb-backend",
    "js-sha256": "^0.9.0",
    "melanke-watchjs": "^1.5.0",
    "multer": "^1.4.1",
    "pubnub": "^4.21.7",
