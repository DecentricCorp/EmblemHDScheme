{
  "name": "emblemdatstorage",
  "version": "1.0.0",
  "description": "Scheme to deterministically derive keys in a hard to reverse way",
  "main": "index.js",
  "scripts": {
    "test": "mocha test",
    "serve": "npm run build; node server.js",
    "build": "browserify ./public/js/src.js -o ./public/js/bundle.js -dv",
    "archiver": "cd archivist; node index.js",
    "postinstall": "git clone https://github.com/DecentricCorp/archivist.git; cd archivist; npm install",
    "docker": "npm run docker-build; npm run docker-run",
    "docker-build": "docker build . -t emblemhd --no-cache",
